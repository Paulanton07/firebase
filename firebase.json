{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "thesocial-radio-app",
          "appId": "1:1013647702646:android:52d80c47690c1be5ef3d57",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "thesocial-radio-app",
          "configurations": {
            "android": "1:1013647702646:android:52d80c47690c1be5ef3d57",
            "web": "1:1013647702646:web:ceb91b094c1d2ca6ef3d57"
          }
        }
      }
    }
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
